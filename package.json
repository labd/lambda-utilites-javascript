{
  "version": "0.5.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@labdigital/lambda-utilities",
  "author": "Bram Kaashoek",
  "module": "dist/lambda-utilities.esm.js",
  "size-limit": [
    {
      "path": "dist/lambda-utilities.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lambda-utilities.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "3.218.0",
    "@aws-sdk/client-sqs": "^3.218.0",
    "@sentry/integrations": "7.15.0",
    "@sentry/serverless": "7.15.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.4.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/koa": "2.11.6",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "koa": "^2.13.1",
    "@opentelemetry/api": "1.4.0"
  }
}
